   "id": "2318d44b-3ee7-4a26-98de-2563ed67f0ee",
   "metadata": {},
   "outputs": [],
   "source": [
    "def rab (n, k = 1):\n",
    "    if n == 1:\n",
    "        return 1\n",
    "    if n == 2:\n",
    "        return 1\n",
    "    return rab(n-1, k) + (rab(n-2, k)*k)"
   ]
  },
